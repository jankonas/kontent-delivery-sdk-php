{
    "name": "kentico/kontent-delivery-sdk-php",
    "type": "library",
    "license": "MIT",
    "description": "Kentico Kontent Delivery SDK for PHP",
    "keywords": [
        "delivery",
        "content-delivery",
        "headless",
        "cms",
        "kentico",
        "kentico-kontent",
        "headless-cms"
    ],
    "homepage": "https://kontent.ai/",
    "authors": [
        {
            "name": "Kentico",
            "email": "devrel@kentico.com"
        },
        {
            "name": "Stephen Rushing",
            "email": "stephen.rushing@esiteful.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "nategood/httpful": "^0.2.20",
        "kub-at/php-simple-html-dom-parser": "^1.9"
    },
    "require-dev": {
        "phpdocumentor/shim": "^3.3",
        "phpunit/phpunit": "^8.5.12"
    },
    "autoload": {
        "psr-4": {
            "Kentico\\Kontent\\": "src/Kentico/Kontent"
        }
    },
    "config": {
        "allow-plugins": {
            "phpdocumentor/shim": true
        }
    }
}
